{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite --host 0.0.0.0",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "lint": "vue-cli-service lint",
    "lint-check": "vue-cli-service lint --no-fix",
    "start": "vite --host 0.0.0.0",
    "dev": "vite --host 0.0.0.0",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e:ci": "vue-cli-service test:e2e --headless",
    "test:unit:debug": "node --inspect-brk=0.0.0.0:9229 ./node_modules/@vue/cli-service/bin/vue-cli-service.js test:unit --no-cache --runInBand",
    "test:unit:watch": "vue-cli-service test:unit --watch"
  },
  "dependencies": {
    "@intlify/core": "9.1.10",
    "@mdi/font": "6.7.96",
    "@react-pdf/renderer": "2.1.1",
    "@sentry/browser": "6.19.7",
    "@sentry/vue": "6.19.7",
    "@tiptap/extension-bold": "2.0.0-beta.26",
    "@tiptap/extension-bubble-menu": "2.0.0-beta.56",
    "@tiptap/extension-bullet-list": "2.0.0-beta.26",
    "@tiptap/extension-document": "2.0.0-beta.15",
    "@tiptap/extension-hard-break": "2.0.0-beta.30",
    "@tiptap/extension-heading": "2.0.0-beta.26",
    "@tiptap/extension-history": "2.0.0-beta.21",
    "@tiptap/extension-italic": "2.0.0-beta.26",
    "@tiptap/extension-list-item": "2.0.0-beta.20",
    "@tiptap/extension-ordered-list": "2.0.0-beta.27",
    "@tiptap/extension-paragraph": "2.0.0-beta.23",
    "@tiptap/extension-placeholder": "2.0.0-beta.48",
    "@tiptap/extension-strike": "2.0.0-beta.27",
    "@tiptap/extension-text": "2.0.0-beta.15",
    "@tiptap/extension-underline": "2.0.0-beta.23",
    "@tiptap/vue-2": "2.0.0-beta.79",
    "@vue/composition-api": "1.6.2",
    "assert": "2.0.0",
    "axios": "0.27.2",
    "check-password-strength": "2.0.5",
    "comlink": "4.3.1",
    "dayjs": "1.11.2",
    "deepmerge": "4.2.2",
    "file-saver": "2.0.5",
    "hal-json-vuex": "2.0.0-alpha.14",
    "html-to-react": "1.4.8",
    "inter-ui": "3.19.3",
    "js-cookie": "3.0.1",
    "lodash": "4.17.21",
    "raf": "3.4.1",
    "react": "17.0.2",
    "slugify": "1.6.5",
    "url-template": "3.0.0",
    "util": "0.12.4",
    "vee-validate": "3.4.14",
    "vite-plugin-worker": "1.0.5",
    "vue": "2.6.14",
    "vue-axios": "3.4.1",
    "vue-i18n": "8.27.1",
    "vue-recaptcha-v3": "1.9.0",
    "vue-router": "3.5.4",
    "vuedraggable": "2.24.3",
    "vuetify": "2.6.6",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/vue": "5.8.3",
    "@vue/cli-plugin-babel": "5.0.4",
    "@vue/cli-plugin-e2e-cypress": "5.0.4",
    "@vue/cli-plugin-eslint": "5.0.4",
    "@vue/cli-plugin-unit-jest": "5.0.4",
    "@vue/cli-service": "5.0.4",
    "@vue/eslint-config-standard": "6.1.0",
    "@vue/test-utils": "1.3.0",
    "@vue/vue2-jest": "27.0.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "coveralls": "3.1.1",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-vue": "7.20.0",
    "flush-promises": "1.0.2",
    "jest-canvas-mock": "2.4.0",
    "jest-serializer-vue-tjw": "3.19.0",
    "lint-staged": "12.4.3",
    "sass": "1.32.13",
    "vite": "2.9.9",
    "vite-plugin-components": "0.13.3",
    "vite-plugin-shim-react-pdf": "1.0.5",
    "vite-plugin-vue2": "2.0.1",
    "vite-plugin-vue2-svg": "0.1.8",
    "vue-cli-plugin-vuetify": "2.5.0",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {
      "vue/component-tags-order": [
        "error",
        {
          "order": [
            "template",
            "script",
            "style"
          ]
        }
      ],
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ],
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 3,
          "multiline": {
            "max": 2,
            "allowFirstLine": true
          }
        }
      ],
      "vue/singleline-html-element-content-newline": [
        "error",
        {
          "ignoreWhenEmpty": true,
          "ignores": [
            "v-chip",
            "v-icon",
            "v-btn",
            "v-alert",
            "span",
            "router-link",
            "h1",
            "pre"
          ]
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/.jest/register-context.js",
      "<rootDir>/.jest/environment.js",
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/.jest/jest-setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue2-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vuetify|vee-validate))"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue-tjw"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)",
      "**/__tests__/*.spec.(js|jsx|ts|tsx)",
      "**/tests/infrastructure/**/*.spec.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "coverageDirectory": "data/coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
