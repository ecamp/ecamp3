<div class="row">
	<center ng-show="isLoading">
		<div id="fountainG" style="margin: 31.5px;">
			<div id="fountainG_1" class="fountainG">
			</div>
			<div id="fountainG_2" class="fountainG">
			</div>
			<div id="fountainG_3" class="fountainG">
			</div>
			<div id="fountainG_4" class="fountainG">
			</div>
			<div id="fountainG_5" class="fountainG">
			</div>
			<div id="fountainG_6" class="fountainG">
			</div>
			<div id="fountainG_7" class="fountainG">
			</div>
			<div id="fountainG_8" class="fountainG">
			</div>
		</div>
	</center>

	<div ng-show="numberOfPages == 0">
		<p align="center">{% block empty_text %}{% endblock %}</p>
	</div>

{% verbatim %}
	<div class="col-sm-6 col-md-4" ng-hide="isLoading" ng-repeat="item in items">
		<a 	href="{{ item._embedded.user._links.web.href }}" class="btn btn-default col-xs-12"
			style="cursor: pointer; margin-bottom: 15px">
			<table style="margin: 0px; padding: 0px; width: 100%">
				<tr>
					<td style="width: 50px">
						<img style="width: 40px"
							 src="https://cdn3.iconfinder.com/data/icons/glyph/227/User-1-256.png" />
					</td>
					<td style="text-align: left">
						<h5 style="display: inline">{{ item._embedded.user.username }}</h5>
						<p>{{ item._embedded.user.displayName }}</p>
					</td>
				</tr>
			</table>
{% endverbatim %}
{% if acl.isAllowed(me, group, 'administrate') %}
			<a 	data-toggle="modal"
				data-backdrop="true"
				data-target="#remoteModalDialog"
				href="{{ editMembershipBaseUrl }}{% verbatim %}{{ item._embedded.user.id }}{% endverbatim %}"
				class="btn btn-noborder btn-xs btn-bottom-right"
			>
				<i class="icon-edit"></i>
			</a>
{% endif %}
		</a>
	</div>
</div>
