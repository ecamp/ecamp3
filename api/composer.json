{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "2.6.5",
        "composer/package-versions-deprecated": "1.11.99",
        "doctrine/annotations": "1.13.2",
        "doctrine/doctrine-bundle": "2.4.2",
        "doctrine/doctrine-migrations-bundle": "3.1.1",
        "doctrine/orm": "2.9.4",
        "exercise/htmlpurifier-bundle": "3.1.0",
        "guzzlehttp/guzzle": "7.3.0",
        "lexik/jwt-authentication-bundle": "2.12.6",
        "nelmio/cors-bundle": "2.1.1",
        "phpdocumentor/reflection-docblock": "5.2.2",
        "stof/doctrine-extensions-bundle": "1.6.0",
        "symfony/asset": "5.3.4",
        "symfony/console": "5.3.6",
        "symfony/dotenv": "5.3.6",
        "symfony/expression-language": "5.3.4",
        "symfony/flex": "1.13.4",
        "symfony/framework-bundle": "5.3.4",
        "symfony/intl": "5.3.4",
        "symfony/mailer": "v5.3.4",
        "symfony/mercure-bundle": "0.3.2",
        "symfony/monolog-bundle": "3.7.0",
        "symfony/property-access": "5.3.4",
        "symfony/property-info": "5.3.4",
        "symfony/runtime": "5.3.4",
        "symfony/security-bundle": "5.3.4",
        "symfony/serializer": "5.3.4",
        "symfony/twig-bundle": "5.3.4",
        "symfony/validator": "5.3.6",
        "symfony/yaml": "5.3.6",
        "webonyx/graphql-php": "14.9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.0.2",
        "hautelook/alice-bundle": "2.9.0",
        "justinrainbow/json-schema": "5.2.11",
        "php-coveralls/php-coveralls": "2.4.3",
        "symfony/browser-kit": "5.3.4",
        "symfony/css-selector": "5.3.4",
        "symfony/debug-bundle": "5.3.4",
        "symfony/http-client": "5.3.4",
        "symfony/maker-bundle": "1.33.0",
        "symfony/phpunit-bridge": "5.3.4",
        "symfony/stopwatch": "5.3.4",
        "symfony/var-dumper": "5.3.6",
        "symfony/web-profiler-bundle": "5.3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*"
    },
    "scripts": {
        "cs-check": "php-cs-fixer fix --dry-run",
        "cs-fix": "php-cs-fixer fix",
        "test": "bin/phpunit -d memory_limit=-1",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
