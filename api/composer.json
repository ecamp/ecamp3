{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "2.6.6",
        "composer/package-versions-deprecated": "1.11.99",
        "cweagans/composer-patches": "1.7.1",
        "doctrine/annotations": "1.13.2",
        "doctrine/doctrine-bundle": "2.5.2",
        "doctrine/doctrine-migrations-bundle": "3.1.1",
        "doctrine/orm": "2.10.3",
        "exercise/htmlpurifier-bundle": "3.1.0",
        "guzzlehttp/guzzle": "7.4.0",
        "lexik/jwt-authentication-bundle": "2.14.1",
        "nelmio/cors-bundle": "2.2.0",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "rize/uri-template": "0.3.4",
        "stof/doctrine-extensions-bundle": "1.7.0",
        "swaggest/json-schema": "0.12.39",
        "symfony/asset": "5.3.11",
        "symfony/console": "5.3.11",
        "symfony/dotenv": "5.3.10",
        "symfony/expression-language": "5.3.11",
        "symfony/flex": "1.17.5",
        "symfony/framework-bundle": "5.3.11",
        "symfony/intl": "5.3.11",
        "symfony/mailer": "5.3.9",
        "symfony/mercure-bundle": "0.3.3",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/property-access": "5.3.8",
        "symfony/property-info": "5.3.8",
        "symfony/runtime": "5.3.11",
        "symfony/security-bundle": "5.3.12",
        "symfony/serializer": "5.3.12",
        "symfony/twig-bundle": "5.3.10",
        "symfony/validator": "5.3.12",
        "symfony/yaml": "5.3.11",
        "webonyx/graphql-php": "14.11.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.3.2",
        "hautelook/alice-bundle": "2.9.0",
        "justinrainbow/json-schema": "5.2.11",
        "php-coveralls/php-coveralls": "2.5.1",
        "phpstan/phpstan": "^1.1",
        "phpunit/phpunit": "9.5.10",
        "symfony/browser-kit": "5.3.4",
        "symfony/css-selector": "5.3.4",
        "symfony/debug-bundle": "5.3.4",
        "symfony/http-client": "5.3.11",
        "symfony/maker-bundle": "1.36.3",
        "symfony/phpunit-bridge": "5.3.11",
        "symfony/stopwatch": "5.3.4",
        "symfony/var-dumper": "5.3.11",
        "symfony/web-profiler-bundle": "5.3.8",
        "vimeo/psalm": "^4.12"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*"
    },
    "scripts": {
        "cs-check": "php-cs-fixer fix --dry-run",
        "cs-fix": "php-cs-fixer fix",
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "bin/phpunit -d memory_limit=-1"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/dependency-injection": "5.3.7",
        "symfony/symfony": "*"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {
            "api-platform/core": {
                "Allow NULL-Links": "patch/api-plattform-allow-null-links.patch"
            }
        }
    }
}
